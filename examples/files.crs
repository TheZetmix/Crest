[!include] {
    "stdio.h"
    "stdlib.h"
}

fn main(args : int, argv : **char) : int {
    var filename : *char;
    if argc > 0 {
        filename = args[1];
    } else {
        printf("file didnt provided\n");
        exit(0);
    }
    var fd : *FILE = fopen(filename, "r");
    if !fd {
        printf("cant open file\n");
        exit(0);
    }
    var c : int;
    while (c = fgetc(fd)) != EOF {
        printf("%c", c);
    }
}
