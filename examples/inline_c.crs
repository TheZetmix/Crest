[!include] {
    "stdio.h"
    "stdlib.h"
}

[!c] {
    int global_a = 10;
    
    void print_global_a() {
        printf("Global: %d\n", global_a);
    }
}

fn main() : int {
    print_global_a(); 
    [!c] {
        int x = 10;
        int y = x - 14;
    }
    printf("Local in main: %d\n", x + y);
}
